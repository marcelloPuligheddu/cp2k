        &SCREENING
          EPS_SCHWARZ 1.0E-10
          #SCREEN_ON_INITIAL_P Y
        &END

        &MEMORY
          MAX_MEMORY 1000
        &END

        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 6.0
          T_C_G_DATA t_c_g.dat
        &END

        FRACTION 0.25
      &END HF
    &END XC
  &END DFT

