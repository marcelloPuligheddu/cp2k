&GLOBAL
  PROJECT MgO_pbe0_energy
  RUN_TYPE energy
  EXTENDED_FFT_LENGTHS .true.
  PRINT_LEVEL medium
&END GLOBAL

&FORCE_EVAL
  METHOD QS

  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    BASIS_SET_FILE_NAME BASIS_MOLOPT_UCL
    BASIS_SET_FILE_NAME BASIS_ADMM
